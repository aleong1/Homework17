public class Coin{
    private double value;
    private String upFace;
    private String name;
    private int flipCtr;
    private int headsCtr;
    private int tailsCtr;
    private double bias;

    public Coin() {
	value = 0.10;
	upFace = "heads";
	name = "dime";
	flipCtr = 0;
	headsCtr = 0;
	tailsCtr = 0;
	bias = 0.0;
	
}
